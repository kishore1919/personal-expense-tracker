    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-firebase-hooks": "^5.1.1",
    "react-icons": "^5.5.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
